<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageNotifiedStudents"
        android:layout_width="295dp"
        android:layout_height="208dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reading" />

    <ImageView
        android:id="@+id/imageAttendanceReport"
        android:layout_width="295dp"
        android:layout_height="199dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageNotifiedStudents"
        app:srcCompat="@drawable/stats" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="295dp"
        android:layout_height="33dp"
        android:text="View Notified Students"
        android:textColor="#000000"
        android:textSize="7pt"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@id/imageNotifiedStudents"
        app:layout_constraintLeft_toLeftOf="@id/imageNotifiedStudents" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="298dp"
        android:layout_height="25dp"
        android:text="View Attendance Report"
        android:textColor="#000000"
        android:textSize="7pt"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@id/imageAttendanceReport"
        app:layout_constraintLeft_toLeftOf="@id/imageAttendanceReport" />

</android.support.constraint.ConstraintLayout>